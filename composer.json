{
  "name": "wild-and-without/blog",
  "description": "Wild and Without Blog",
  "type": "project",
  "license": "proprietary",
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org"
    }
  ],
  "config": {
    "platform": {
      "php": "7.4"
    },
    "allow-plugins": {
      "composer/installers": true,
      "johnpbloch/wordpress-core-installer": true
    }
  },
  "require": {
    "johnpbloch/wordpress": "^6.0",
    "wpackagist-theme/twentynineteen": "^3.0",
    "wpackagist-plugin/akismet": "^5",
    "wpackagist-plugin/better-wp-security": "^9.0",
    "wpackagist-plugin/wordpress-seo": "^24.0",
    "wpackagist-plugin/contact-form-7": "^6.0",
    "wpackagist-plugin/updraftplus": "^1.13",
    "wpackagist-plugin/featured-images-for-rss-feeds": "^1.4",
    "wpackagist-plugin/mailchimp-for-wp": "^4.1",
    "wpackagist-plugin/media-file-renamer": "^6",
    "wpackagist-plugin/advanced-custom-fields": "^6.0",
    "wpackagist-plugin/widget-css-classes": "^1.5",
    "wpackagist-plugin/disable-embeds": "^1.3",
    "wpackagist-plugin/wp-mail-smtp": "^4.0",
    "wpackagist-plugin/enable-media-replace": "^4.0",
    "wpackagist-plugin/widget-context": "^1.1",
    "wpackagist-plugin/transients-manager": "^2.0",
    "wpackagist-plugin/pre-publish-checklist": "^1.1",
    "wpackagist-plugin/broken-link-checker": "^2.0",
    "wpackagist-plugin/disable-xml-rpc": "^1.0",
    "wpackagist-plugin/custom-taxonomy-order-ne":"^4.0",
    "wpackagist-plugin/google-site-kit":"^1.0",
    "wpackagist-plugin/instagram-feed":"^6.0",
    "wpackagist-plugin/redis-cache":"^2.2.2",
    "wpackagist-plugin/classic-widgets":"^0.3",
    "wpackagist-plugin/cloudflare":"^4.11.0",
    "wpackagist-plugin/simply-gallery-block":"^3.2.5",
    "johnbillion/wp-crontrol":"^1.10",
    "roots/wp-password-bcrypt": "^1.0",
    "vlucas/phpdotenv": "^5.2",
    "raiym/instagram-php-scraper": "^0.12.0",
    "guzzlehttp/guzzle": "^7.2",
    "symfony/cache": "^5.2"
  },
  "extra" : {
    "wordpress-install-dir": "public/wp",
    "installer-paths": {
      "public/wp-content/mu-plugins/{$name}/": ["type:wordpress-muplugin"],
      "public/wp-content/plugins/{$name}/": ["type:wordpress-plugin"],
      "public/wp-content/themes/{$name}/": ["type:wordpress-theme"]
    }
  }
}
