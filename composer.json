{
  "name": "wild-and-without/blog",
  "description": "Wild and Without Blog",
  "type": "project",
  "license": "proprietary",
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org"
    }
  ],
  "config": {
    "platform": {
      "php": "7.4"
    },
    "allow-plugins": {
      "composer/installers": true,
      "johnpbloch/wordpress-core-installer": true
    }
  },
  "require": {
    "johnpbloch/wordpress": "^5.0",
    "wpackagist-theme/twentynineteen": "^2.0",
    "wpackagist-plugin/akismet": "^4",
    "wpackagist-plugin/better-wp-security": "^8.0",
    "wpackagist-plugin/wordpress-seo": "^17.0",
    "wpackagist-plugin/google-analytics-dashboard-for-wp": "^7.0",
    "wpackagist-plugin/contact-form-7": "^5.0",
    "wpackagist-plugin/updraftplus": "^1.13",
    "wpackagist-plugin/featured-images-for-rss-feeds": "^1.4",
    "wpackagist-plugin/mailchimp-for-wp": "^4.1",
    "wpackagist-plugin/media-file-renamer": "^5",
    "wpackagist-plugin/advanced-custom-fields": "^5.8",
    "wpackagist-plugin/widget-css-classes": "^1.5",
    "wpackagist-plugin/disable-embeds": "^1.3",
    "wpackagist-plugin/wp-mail-smtp": "^3.0",
    "wpackagist-plugin/enable-media-replace": "^3.2",
    "wpackagist-plugin/widget-context": "^1.1",
    "wpackagist-plugin/transients-manager": "^2.0",
    "wpackagist-plugin/pre-publish-post-checklist": "^3.1",
    "wpackagist-plugin/broken-link-checker": "^1.11",
    "wpackagist-plugin/disable-xml-rpc": "^1.0",
    "wpackagist-plugin/custom-taxonomy-order-ne":"^3.0",
    "wpackagist-plugin/google-site-kit":"^1.0",
    "wpackagist-plugin/instagram-feed":"^2.9",
    "johnbillion/wp-crontrol":"^1.10",
    "roots/wp-password-bcrypt": "^1.0",
    "vlucas/phpdotenv": "^5.2",
    "raiym/instagram-php-scraper": "^0.12.0",
    "guzzlehttp/guzzle": "^7.2",
    "symfony/cache": "^5.2",
    "humanmade/s3-uploads": "^3.0"
  },
  "extra" : {
    "wordpress-install-dir": "public/wp",
    "installer-paths": {
      "public/wp-content/mu-plugins/{$name}/": ["type:wordpress-muplugin"],
      "public/wp-content/plugins/{$name}/": ["type:wordpress-plugin"],
      "public/wp-content/themes/{$name}/": ["type:wordpress-theme"]
    }
  }
}
